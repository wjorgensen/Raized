{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "bns-get-namespace-price-response",
  "title": "BnsGetNamespacePriceResponse",
  "description": "Fetch price for namespace.",
  "required": [ "units", "amount" ],
  "type": "object",
  "properties": {
    "units": {
        "type": "string"
    },
    "amount": {
        "type": "string",
        "pattern": "^[0-9]+$"
    }
  }
}

